<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.uca.network.extension.mapper.allocations.MacAddressMapper">

    <resultMap id="macAddressMap" type="com.uca.network.extension.mapper.allocations.model.MacAddress">
        <result column="mac_address" jdbcType="VARCHAR" property="macAddress"/>
        <result column="mac_source" jdbcType="VARCHAR" property="macSource"/>
    </resultMap>

    <insert id="insert" parameterType="com.uca.network.extension.mapper.allocations.model.MacAddress"
            useGeneratedKeys="true" keyProperty="id">
        insert into tbl_mac_address(mac_address, mac_source)
        values(#{macAddress}, #{macSource})
    </insert>

    <delete id="delete">
        delete from tbl_mac_address where mac_address = #{macAddress}
    </delete>


</mapper>

